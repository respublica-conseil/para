= modal.body do
  .alert.alert-success
    = model_translate(:success, scope: "para.jobs", model: job)

  = javascript_tag do
    :plain
      window.location.href = '#{ job.file.attachment_url }';

= modal.footer do
  %button.btn.btn-default{ data: { action: 'click->para-admin-modal#closeModal' } }
    = t('para.shared.close')
